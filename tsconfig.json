{
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "noEmit": true
  },
  "include": ["guest-js/*.ts"],
  "exclude": ["dist-js", "node_modules"]
}
